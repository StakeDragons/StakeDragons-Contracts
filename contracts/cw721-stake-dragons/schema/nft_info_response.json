{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "NftInfoResponse",
  "type": "object",
  "properties": {
    "extension": {
      "description": "You can add any custom metadata here when you extend cw721-base",
      "anyOf": [
        {
          "$ref": "#/definitions/Metadata"
        },
        {
          "type": "null"
        }
      ]
    },
    "token_uri": {
      "description": "Universal resource identifier for this NFT Should point to a JSON file that conforms to the ERC721 Metadata JSON Schema",
      "type": [
        "string",
        "null"
      ]
    }
  },
  "definitions": {
    "Metadata": {
      "type": "object",
      "properties": {
        "animation_url": {
          "type": [
            "string",
            "null"
          ]
        },
        "attributes": {
          "anyOf": [
            {
              "$ref": "#/definitions/Trait"
            },
            {
              "type": "null"
            }
          ]
        },
        "background_color": {
          "type": [
            "string",
            "null"
          ]
        },
        "description": {
          "type": [
            "string",
            "null"
          ]
        },
        "external_url": {
          "type": [
            "string",
            "null"
          ]
        },
        "image": {
          "type": [
            "string",
            "null"
          ]
        },
        "image_data": {
          "type": [
            "string",
            "null"
          ]
        },
        "name": {
          "type": [
            "string",
            "null"
          ]
        },
        "youtube_url": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "Trait": {
      "type": "object",
      "required": [
        "accessory",
        "background",
        "base",
        "name"
      ],
      "properties": {
        "accessory": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "background": {
          "type": "string"
        },
        "base": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      }
    }
  }
}
